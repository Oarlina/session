{% extends 'base.html.twig' %}

{% block title %} Détail de {{user.names}} {% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h4> Détail  de {{user.names}} :</h4>
    <br>
    <p>
        <u>Nom complets :</u> {{user.names}} <br>
        <u>Ville :</u> {{user.city}} <br>
        <u>Email :</u> {{user.email}} <br>
        <u>Numéro de téléphone :</u> {{user.phoneNumber}} <br>
    </p>
    <h4>Session participé :</h4>
    
</div>
{% endblock %}